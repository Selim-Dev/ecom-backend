const mongoose = require('mongoose');
// const SubCategory = require('./SubCategory');
const categorySchema = new mongoose.Schema({
    name: {
        type: String,
        required: true
    },
    photo: {
        type: String
    },
    subCategories: [
        {
            type: mongoose.Schema.ObjectId,
            ref: 'SubCategory'
        }
    ]
});

exports.Category = mongoose.model('Category', categorySchema);
